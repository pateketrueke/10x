{
  "name": "10x",
  "version": "0.0.0",
  "bin": {
    "10x": "bin/cli"
  },
  "scripts": {
    "dev": "bun run build && bun --watch run index.js",
    "watch": "bun test --watch tests/",
    "build": "bun build src/main.esm.js --outdir=dist --format=esm --naming=[name].js --external=fs --external=os --external=tty --external=path --external=chalk --external=convert-units --external=currency-symbol.js",
    "pretest": "eslint tests *.js src/lib",
    "test": "bun test tests/",
    "test:unit": "TZ=UTC bun test tests/",
    "test:ci": "TZ=UTC bun test --coverage tests/"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.1",
    "stdout-stderr": "^0.1.13"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "convert-units": "^2.3.4",
    "currency-symbol.js": "^1.0.0",
    "fs-extra": "^9.0.0",
    "mock-stdin": "^1.0.0",
    "prompt-sync": "^4.2.0",
    "prompt-sync-history": "^1.0.1",
    "prompts": "^2.3.1",
    "testdouble": "^3.13.1",
    "timekeeper": "^2.2.0",
    "wargs": "^0.9.0"
  }
}
